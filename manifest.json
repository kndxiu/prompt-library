{
  "manifest_version": 3,
  "name": "Prompt Library",
  "version": "1.0.2",
  "description": "A library of prompts for AI chats",
  "permissions": ["activeTab", "storage", "scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content/index.tsx"],
      "run_at": "document_idle"
    }
  ]
}
